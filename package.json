{
	"name": "marcdonald.com",
	"license": "MIT",
	"author": "Marc Donald",
	"repository": "https://github.com/MarcDonald/marcdonald.com",
	"bugs": "https://github.com/MarcDonald/marcdonald.com/issues",
	"homepage": "https://marcdonald.com",
	"private": true,
	"sideEffects": false,
	"scripts": {
		"build": "npm run sass:build && remix build",
		"dev": "concurrently \"npm run sass\" \"remix dev\"",
		"lint:staged": "lint-staged --allow-empty",
		"husky:prepare": "husky install",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"sass": "sass --watch app/:app/",
		"sass:build": "sass app/:app/"
	},
	"dependencies": {
		"@remix-run/node": "1.13.0",
		"@remix-run/react": "1.13.0",
		"@remix-run/vercel": "1.13.0",
		"@vercel/node": "2.9.6",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-feather": "2.0.10"
	},
	"devDependencies": {
		"@marcdonald/eslint-config": "1.2.0",
		"@marcdonald/prettier-config": "1.0.0",
		"@remix-run/dev": "1.13.0",
		"@remix-run/eslint-config": "1.13.0",
		"@remix-run/serve": "1.13.0",
		"@types/react": "18.0.28",
		"@types/react-dom": "18.0.11",
		"@typescript-eslint/eslint-plugin": "5.52.0",
		"@typescript-eslint/parser": "5.52.0",
		"concurrently": "7.6.0",
		"eslint": "8.34.0",
		"husky": "8.0.3",
		"lint-staged": "13.1.2",
		"prettier": "2.8.4",
		"prettier-plugin-tailwindcss": "0.2.3",
		"sass": "1.58.3",
		"typescript": "4.9.5"
	},
	"engines": {
		"node": ">=16"
	}
}
