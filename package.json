{
	"name": "marcdonald.com",
	"license": "MIT",
	"author": "Marc Donald",
	"repository": "https://github.com/MarcDonald/marcdonald.com",
	"bugs": "https://github.com/MarcDonald/marcdonald.com/issues",
	"homepage": "https://marcdonald.com",
	"private": true,
	"sideEffects": false,
	"scripts": {
		"build": "run-s \"build:*\"",
		"build:css": "npm run generate:css -- --minify",
		"build:remix": "remix build",
		"dev": "run-p \"dev:*\"",
		"dev:css": "npm run generate:css -- --watch",
		"dev:remix": "remix dev",
		"format": "prettier --check --ignore-path .gitignore .",
		"format:fix": "prettier --check --write --ignore-path .gitignore .",
		"generate:css": "npx tailwindcss -i app/styles/app.css -o ./app/tailwind.css",
		"husky:prepare": "husky install",
		"lint": "eslint --ignore-path .gitignore .",
		"lint:fix": "eslint --ignore-path .gitignore --fix .",
		"lint:staged": "lint-staged --allow-empty",
		"test": "echo \"TODO test\""
	},
	"dependencies": {
		"@icons-pack/react-simple-icons": "^6.3.0",
		"@remix-run/node": "1.13.0",
		"@remix-run/react": "1.13.0",
		"@remix-run/vercel": "1.13.0",
		"@vercel/node": "2.9.6",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-feather": "2.0.10"
	},
	"devDependencies": {
		"@marcdonald/eslint-config": "1.2.0",
		"@marcdonald/prettier-config": "1.0.0",
		"@remix-run/dev": "1.13.0",
		"@remix-run/eslint-config": "1.13.0",
		"@remix-run/serve": "1.13.0",
		"@types/react": "18.0.28",
		"@types/react-dom": "18.0.11",
		"@typescript-eslint/eslint-plugin": "5.52.0",
		"@typescript-eslint/parser": "5.52.0",
		"clsx": "1.2.1",
		"eslint": "8.34.0",
		"husky": "8.0.3",
		"lint-staged": "13.1.2",
		"npm-run-all": "4.1.5",
		"prettier": "2.8.4",
		"prettier-plugin-tailwindcss": "0.2.3",
		"tailwind-merge": "1.8.1",
		"tailwindcss": "^3.2.7",
		"typescript": "4.9.5"
	},
	"engines": {
		"node": ">=16"
	}
}
